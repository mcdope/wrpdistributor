    "type": "project",
    "require": {
        "php": "^8.2",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "vlucas/phpdotenv": "^5.5",
        "phpseclib/phpseclib": "^3.0",
        "monolog/monolog": "^3.3",
        "symfony/console": "^6.2",
        "doctrine/migrations": "^3.6"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "vimeo/psalm": "^5.6",
        "squizlabs/php_codesniffer": "^3.7",
        "friendsofphp/php-cs-fixer": "^3.14"
    },
    "license": "AGPLv3",
    "autoload": {
